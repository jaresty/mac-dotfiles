^multi [<user.text>]:
  key("cmd-shift-m")
  sleep(50ms)
  insert(text or "")

^multi share [<user.text>]:
  key("ctrl-alt-cmd-shift-e")
  sleep(50ms)
  insert(text or "")

^multi leave:
  key("cmd-shift-escape")

^multi toggle audio:
  key("ctrl-alt-cmd-shift-a")

^multi toggle audio:
  key("ctrl-alt-cmd-shift-a")

^multi toggle video:
  key("ctrl-alt-cmd-shift-c")
